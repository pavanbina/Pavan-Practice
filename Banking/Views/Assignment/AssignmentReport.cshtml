<h2 style="text-align:center">AssignmentReport</h2>
<div class="container">
    <div class="row" id="divtbl">
        <div class="col-xl-12">
            <table id="tbl"></table>
        </div>
    </div>

    <div class="row">
        <div class="panel">
            <div class="panel-heading">
                <h1>Update Due Date</h1>
            </div>
            <div class="panel-body" style="padding:0 30px">
                <div class="row">
                    <div class="form-group">

                        <label>Please select week</label>
                        <br />
                        <select class="form-control select2" id="ddlweek" style="width:30%"> </select>


                    </div>
                </div>
                <div class="row">
                    <table class="tabel table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>COA Date</th>
                                <th>DSA Date</th>
                                <th>NSA Date</th>
                                <th>OS Date</th>
                                <th>PCS Date</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td><input type="date" id="coadate" class="form-control" /></td>
                                <td><input type="date" id="dsadate" class="form-control" /></td>
                                <td><input type="date" id="nsadate" class="form-control" /></td>
                                <td><input type="date" id="osdate" class="form-control" /></td>
                                <td><input type="date" id="pcsdate" class="form-control" /></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
